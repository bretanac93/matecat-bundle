{
    "name": "bretanac93/matecat-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Symfony Matecat bridge",
    "keywords": ["Matecat", "matecat api", "matecat rest api"],
    "authors": [
        {
            "name": "Cesar Bretana Gonzalez",
            "email": "bretanac@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lengoo\\MateCatBundle\\": ""
        }
    },
    "exclude-from-classmap": [
        "/Tests/"
    ],
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/guzzle": "~6.0",
        "jms/serializer-bundle": "^2.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.0",
        "phpunit/phpunit": "^6.4"
    },
    "extra": {
        "branch-alias": null
    }
}
